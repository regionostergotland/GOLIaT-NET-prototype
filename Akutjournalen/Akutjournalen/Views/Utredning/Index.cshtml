@*Arbete pågår med att ersätta nuvarande databas med openEHR...

    Kopiera denna länk för att komma till detaljvyn för ett underlag (Vladimir Dalton):

    <p><a>http://goliatdemoutvtest.lio.se/Utredning/SpecificUnderlag?id=1504707713&EHRID=b51f3709-eeb6-49ed-afae-ef1468ec03fb&formName=Beslut%20v2%20om%20kirurgi%20m%20citations%203&formVersion=1.0.3</a></p>*@


<div ng-app="underlag">
    <div id=" top-row">
        <div id="left-top-row-content">
            <div>
                @*<h4>
                    Listade Beslutsunderlag
                </h4>*@
            </div>
        </div>
        <div id="right-top-row-content">
            <div><h4></h4></div>

        </div>
    </div>

    <div ng-controller="StartSidaUnderlagCtrl">
        <table datatable="ng" class="table table-hover listview" id="show-remisser">
            <thead>
                <tr>
                    <th ng-if="'@HttpContext.Current.Session["ROLE"]' == 'Kirurg' ">Namn</th>
                    <th ng-if="'@HttpContext.Current.Session["ROLE"]' == 'Ledning' ">Kön</th>
                    <th ng-if="'@HttpContext.Current.Session["ROLE"]' == 'Ledning' ">Ålder</th>
                    <th ng-if="'@HttpContext.Current.Session["ROLE"]' == 'Kirurg' ">Personnummer</th>
                    <th>Diagnos</th>
                    <th>Skapat</th>
                    <th>Tid till behandling</th>
                    <th>SVF</th>
                    <th ng-if="'@HttpContext.Current.Session["ROLE"]' == 'Ledning' ">Ansvarig enhet</th>
                    @*<th>Utrednings-ID</th>*@
                    <th>Status</th>
                </tr>
            </thead>

            @*'Beslut, kirurgisk åtgärd med citat 4', '1.0.4'*@
            <tbody>
                <tr ng-repeat="underlag in underlagen" id="underlag.ehrid" ng-click="pickUnderlag('@Url.Action("Index","Utredning/SpecificUnderlag")', 'Beslut v2 om kirurgi m citations 3', '1.0.8' ,underlag.ehrid, underlag._id.Timestamp, '@HttpContext.Current.Session["ROLE"]', underlag.diagnoskod)">
                    <td ng-if="'@HttpContext.Current.Session["ROLE"]' == 'Ledning' ">{{underlag.gender || "-"}}</td>
                    <td ng-if="'@HttpContext.Current.Session["ROLE"]' == 'Ledning' ">{{underlag.personnummer || "-"}}</td>
                    <td ng-if="'@HttpContext.Current.Session["ROLE"]' == 'Kirurg' ">{{underlag.firstname || "-"}} {{underlag.lastname || "-"}}</td>
                    <td ng-if="'@HttpContext.Current.Session["ROLE"]' == 'Kirurg' ">{{underlag.personnummer || "-"}}</td>
                    <td>{{underlag.diagnoskod || "-"}}</td>
                    <td>{{underlag.time_created || "-"}}</td>

                    <td><b>{{underlag.start_date_countdown || "-"}}</b></td>
                    <td>{{underlag.SVF ||'-'}}</td>
                    <td ng-if="'@HttpContext.Current.Session["ROLE"]' == 'Ledning' ">{{underlag.ansvarig_enhet || "-"}}</td>
                    @*<td>{{underlag.utredning_id || "-"}}</td>*@
                    <td>{{ underlag.status || "-" }} </td>
                </tr>
            </tbody>
        </table>
    </div>
</div>